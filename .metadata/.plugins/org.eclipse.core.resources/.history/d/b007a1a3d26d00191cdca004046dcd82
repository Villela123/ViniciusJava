package br.com.usarclasses;

import com.loja.Categoria;

public class GerenciarProdutos{
	public static void main(String[] args){
		/* Vamos criar um vetor (Array/Colecao) de Categorias
		 * 
		 */
		Categoria[] cat = {
						new Categoria(11, "Informatica", "Tecnologia"),
						new Categoria(12, "Suprimentos", "Diversos"),
						new Categoria(13, "Escolar", "Para estudo")
		};		
		for(int i = 0 ; i < cat.length ; i++)
			System.out.println(cat[i].getNome());
		}
}
